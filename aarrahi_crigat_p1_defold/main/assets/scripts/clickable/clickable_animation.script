local initial_scale
local m_scale
go.property("mScale", 5)
function init(self)
	initial_scale = go.get_scale()
	m_scale = self.mScale
end

function on_message(self, message_id, message, sender)
	if message_id == hash("click") then
		
		animate()
	end
end

function animate()
	go.set_scale(initial_scale)
	go.animate(".", "scale", go.PLAYBACK_ONCE_PINGPONG, go.EASING_OUTSINE / m_scale, 1, 0.5)
end